{
  "name": "goshop-monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=packages/web",
    "dev:api": "npm run dev --workspace=packages/api",
    "build": "npm run build --workspace=packages/web",
    "build:api": "npm run build --workspace=packages/api",
    "start": "npm run start --workspace=packages/api"
  },
  "devDependencies": {
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.2",
    "octokit": "^5.0.5"
  }
}
